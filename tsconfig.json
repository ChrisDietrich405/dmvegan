{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/(tabs)/create-account.jsx",
		"app/(tabs)/allow-geolocation.jsx",
		"app/(tabs)/add-event.jsx",
		"app/(tabs)/index.jsx",
		"app/+not-found.jsx",
		"app/_layout.jsx",
		"app/(tabs)/explore.jsx",
		"app/(tabs)/_layout.jsx",
		"app/(tabs)/recover-password.jsx"
	]
}
